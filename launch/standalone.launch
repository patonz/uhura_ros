<launch>
  <arg name="UAV_NAME" default="uav1" />
  <arg name="RUN_TYPE" default="" />
  <group ns="$(arg UAV_NAME)">
    <node pkg="uhura_ros" name="uhuranode" type="uhura.py" output="screen">
      <param name="UAV_NAME" type="string" value="$(arg UAV_NAME)" />
      <param name="RUN_TYPE" type="string" value="$(arg RUN_TYPE)" />
      <param name="baudrate" type="int" value="9600" />
    </node>
  </group>
</launch> 